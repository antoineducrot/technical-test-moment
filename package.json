{
  "name": "technical-test-moment",
  "version": "0.0.1",
  "author": "Antoine DUCROT",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push",
    "db:migrate-deploy": "turbo db:migrate-deploy",
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:db:start": "./docker/pg-admin/generate-servers.sh && docker-compose -f docker-compose.dev.yml up --build -d",
    "dev:db:stop": "docker-compose -f docker-compose.dev.yml down",
    "prod:start": "docker-compose -f docker-compose.yml up --build -d",
    "prod:stop": "docker-compose -f docker-compose.yml down",
    "start": "turbo start",
    "lint": "turbo lint",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "prettier --write .",
    "prepare": "husky || true",
    "cspell": "cspell --no-summary --no-progress .",
    "commit": "cz",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@repo/eslint-config": "*",
    "cspell": "^8.14.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "turbo": "^2.1.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
